@import './vars.scss';

.back-stage-table.el-table {
    .cols-list {
        .cols-item {
            font-size: 14px;
        }

        .item-detail.not-link {
            color: #999;
            cursor: default;
        }
    }
}
@each $color-key, $color-value in $color-data-set {
    .theme-#{$color-key} {
        .operate {
            &:hover {
                color: nth($color-value, 1);
            }
        }
    }
}
